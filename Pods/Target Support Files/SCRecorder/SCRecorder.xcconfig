GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/SCRecorder" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/SCRecorder"
OTHER_LDFLAGS = -framework "AVFoundation" -weak_framework "GLKit" -weak_framework "Metal"
